<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #6b0000;" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.fxml.AddCityToSupplier">
